                <form action='/upload' method="post">

        <div class="food">
        <div class="pulse card">
            <h3>Pulse</h3>
            <input type="checkbox" name="Moong Dal" value="Moong Dal"> Moong Dal
            <input type="checkbox" name="Dal Tadka" value="Dal Tadka"> Dal Tadka<br><br>
            <input type="checkbox" name=" Tuar Dal" value=" Tuar Dal"> Tuar Dal
            <input type="checkbox" name="Chana Dal" value="Chana Dal"> Chana Dal
        </div>
        <div class="pureVeg card">
                <h3>Pure Veg</h3>
                <input type="checkbox" name="Rajma" value="Rajma"> Rajma
                <input type="checkbox" name="Sev Curry" value="Sev Curry"> Sev Curry<br><br>
                <input type="checkbox" name="Aloo Sabzi" value="Aloo Sabzi"> Aloo Sabzi
                <input type="checkbox" name="Paneer Gravy" value="Paneer Gravy"> Paneer Gravy
            </div>
            <div class="rice card">
                    <h3>Rice</h3>
                    <input type="checkbox" name="Veg Biryani" value="Veg Biryani"> Veg Biryani
                    <input type="checkbox" name="Plain Rice" value="Plain Rice">Plain Rice<br><br>
                    <input type="checkbox" name="Jeera Rice" value="Jeera Rice"> Jeera Rice
                    <input type="checkbox" name="Fried Rice" value="Fried Rice"> Fried Rice
                </div>

                <div class="chapati card">
                        <h3>Chapati</h3>
                        <input type="checkbox" name="Plain Roti" value="Plain Roti"> Plain Roti
                        <input type="checkbox" name="Stuffed Paratha" value="Stuffed Paratha">Stuffed Paratha<br><br>
                        <input type="checkbox" name="Tandoori Naan" value="Tandoori Naan"> Tandoori Naan
                        <input type="checkbox" name="Aloo Paratha" value="Aloo Paratha"> Aloo Paratha
                    </div>
                    
            <div class="soups card">
                    <h3>Soups & Salads</h3>
                    <input type="checkbox" name="Green Salad" value="Green Salad"> Green Salad
                    <input type="checkbox" name="Grain Salad" value="Grain Salad">Grain Salad<br><br>
                    <input type="checkbox" name="Tomato Soup" value="Tomato Soup"> Tomato Soup
                    <input type="checkbox" name="Soup" value="Soup"> Soup
                </div>
                
            <div class="wafers card">
                    <h3>Wafers</h3>
                    <input type="checkbox" name="Masala Papad" value="Masala Papad"> Masala Papad
                    <input type="checkbox" name="Kurkure" value="Kurkure">Kurkure<br><br>
                    <input type="checkbox" name="Papad" value="Papad"> Papad
                    <input type="checkbox" name="Masala Wafers"> Masala Wafers
                </div>
                
            <div class="desert card">
                    <h3>Desert</h3>
                    <input type="checkbox" name="Gulab Jamun" value="Gulab Jamun"> Gulab Jamun
                    <input type="checkbox" name="Moong Halwa" value="Moong Halwa">Moong Halwa<br><br>
                    <input type="checkbox" name="Ice cream" value="Ice cream"> Ice cream
                    <input type="checkbox" name=" Sweets" value=" Sweets"> Sweets
                </div>

               <input name="Array" value=<%=submitted%> disabled> 
            <button type="submit" class='btn btn-danger' onclick="giveCheckbox()" >Upload Menu</button>    
</div>
